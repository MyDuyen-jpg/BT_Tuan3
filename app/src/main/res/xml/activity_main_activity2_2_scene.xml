<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="2000"
        motion:autoTransition="none"
        motion:staggered="0.2"
        motion:motionInterpolator="easeIn">
       <KeyFrameSet>
           <KeyCycle
               motion:motionTarget="@+id/imageView6"
               motion:framePosition="0"
               motion:wavePeriod="1"
               android:rotation="15"
               motion:waveOffset="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/textView"
               motion:framePosition="8"
               android:alpha="0" />
       </KeyFrameSet>
        <OnSwipe
            motion:touchAnchorId="@+id/imageView2"
            motion:dragDirection="dragDown" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/textView"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="512dp"
            motion:layout_constraintHorizontal_bias="0.49">
            <CustomAttribute
                motion:attributeName="textColor"
                motion:customColorValue="#ffffffff" />
        </Constraint>
        <Constraint
            android:id="@+id/imageView4"
            android:layout_width="30dp"
            android:layout_height="34dp"
            android:layout_marginStart="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp">
            <CustomAttribute
                motion:attributeName="backgroundColor"
                motion:customColorValue="#9765C8" />
        </Constraint>
        <Constraint
            motion:layout_constraintVertical_bias="0.0"
            android:layout_height="838dp"
            motion:layout_constraintStart_toStartOf="@+id/view"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="@+id/view"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="@+id/view"
            android:layout_width="462dp"
            android:id="@+id/imageView2" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/imageView6"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="34dp"
            android:layout_height="43dp"
            android:layout_marginEnd="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="656dp"
            android:layout_marginRight="16dp" />
        <Constraint
            android:id="@+id/imageView3"
            android:layout_width="97dp"
            android:layout_height="112dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView2"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" />
        <Constraint
            android:id="@+id/view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="411dp"
            android:layout_height="168dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteY="564dp" />
        <Constraint
            android:id="@+id/view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="250dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            motion:layout_constraintVertical_bias="1.0"
            android:layout_height="129dp"
            motion:layout_constraintStart_toStartOf="@+id/view"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="@+id/view"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="@+id/view"
            android:layout_width="410dp"
            android:id="@+id/imageView2"
            android:scaleX="9"
            motion:layout_editor_absoluteY="602dp" />
        <Constraint
            android:layout_height="22dp"
            android:layout_marginBottom="16dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="252dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="108dp"
            android:layout_marginStart="108dp"
            android:id="@+id/imageView7" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="32dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/textView" >
            <CustomAttribute
                motion:attributeName="textColor"
                motion:customColorValue="#2741D5" />
        </Constraint>
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginBottom="48dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="108dp"
            android:layout_marginStart="108dp"
            android:id="@+id/imageView9"
            android:scaleX="0.6"
            android:scaleY="0.6" />
        <Constraint
            android:id="@+id/imageView8"
            motion:layout_constraintStart_toEndOf="@+id/imageView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView9"
            android:scaleX="0.6"
            android:scaleY="0.6" />
        <Constraint
            android:id="@+id/imageView11"
            motion:layout_constraintStart_toEndOf="@+id/imageView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView8"
            android:scaleX="0.6"
            android:scaleY="0.6" />
        <Constraint
            android:id="@+id/imageView10"
            motion:layout_constraintStart_toEndOf="@+id/imageView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView11"
            android:scaleY="0.6"
            android:scaleX="0.6" />
        <Constraint
            android:id="@+id/textView2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="393dp"
            android:layout_height="385dp"
            motion:layout_constraintBottom_toTopOf="@+id/imageView2"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintHorizontal_bias="0.55"
            motion:layout_editor_absoluteY="88dp"
            motion:layout_editor_absoluteX="33dp" />
        <Constraint
            android:layout_height="34dp"
            android:layout_marginTop="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="30dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:id="@+id/imageView4" >
            <CustomAttribute
                motion:attributeName="backgroundColor"
                motion:customColorValue="#ffff" />
        </Constraint>
    </ConstraintSet>
</MotionScene>